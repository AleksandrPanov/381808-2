using namespace std;
struct Iterator
{
	Node *it;
	pair<int, string>& operator*();
	pair<int, string>& operator->();
	void operator++();
}
class MyList
{
	Node *first;
	Node *end;
public:	
	void push_back_current_level(sting data);
	void push_back_next_level(string data);
	pair<int, string> pop();
	void print();
	~MyList();
}
struct Node
{
	string data;
	Node *next;
	Node *down;
	int level;
}
cout << level << ". " << data << "\n";
